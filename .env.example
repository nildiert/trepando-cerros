# Copy to .env and fill in your Strava access token
STRAVA_ACCESS_TOKEN=your-token-here
STRAVA_CLIENT_ID=your-client-id
STRAVA_CLIENT_SECRET=your-client-secret
# Optional: override the OAuth callback URL if different from default
STRAVA_REDIRECT_URI=http://localhost:3000/auth/strava/callback
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_REDIRECT_URI=http://localhost:3000/auth/google_oauth2/callback
